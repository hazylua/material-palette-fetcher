{
  "name": "node-material-palette-fetcher",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "live": "nodemon ./bin/www",
    "tailwind:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "tailwind:live": "npx tailwindcss -i public/stylesheets/tailwind.css -o public/stylesheets/style.css --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "material-color-tool": "^1.1.0",
    "morgan": "~1.9.1",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.22"
  }
}
